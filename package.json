{
  "name": "bare-dev",
  "version": "0.13.3",
  "description": "Development tooling for Bare",
  "main": "index.js",
  "bin": {
    "bare-dev": "bin/bare-dev.js"
  },
  "files": [
    "index.js",
    "bin",
    "lib",
    "cmake",
    "compat"
  ],
  "exports": {
    ".": "./index.js",
    "./android": "./lib/android.js",
    "./build": "./lib/build.js",
    "./bundle": "./lib/bundle.js",
    "./clean": "./lib/clean.js",
    "./configure": "./lib/configure.js",
    "./dependencies": "./lib/dependencies.js",
    "./drive": "./lib/drive.js",
    "./init": "./lib/init.js",
    "./install": "./lib/install.js",
    "./ios": "./lib/ios.js",
    "./paths": "./lib/paths.js",
    "./test": "./lib/test.js",
    "./vendor": "./lib/vendor.js"
  },
  "imports": {
    "child_process": {
      "bare": "bare-subprocess",
      "default": "child_process"
    },
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "fs/*": {
      "bare": "bare-fs/*",
      "default": "fs/*"
    },
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    }
  },
  "scripts": {
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-dev.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-dev/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-dev#readme",
  "dependencies": {
    "bare-bundle": "^1.1.0",
    "bare-fs": "^2.1.1",
    "bare-os": "^2.1.1",
    "bare-path": "^2.1.0",
    "bare-process": "^2.0.0",
    "bare-subprocess": "^3.0.0",
    "bare-which": "^1.1.0",
    "cmake-runtime": "^3.29.1",
    "commander": "^11.1.0",
    "corestore": "^6.4.3",
    "decompress": "^4.2.1",
    "drive-bundler": "^1.5.0",
    "glob": "^10.3.10",
    "hypercore-id-encoding": "^1.0.2",
    "hyperdrive": "^11.0.1",
    "hyperswarm": "^4.3.7",
    "include-static": "^1.1.0",
    "localdrive": "^1.11.0",
    "unix-path-resolve": "^1.0.2"
  },
  "devDependencies": {
    "standard": "^17.0.0"
  }
}
